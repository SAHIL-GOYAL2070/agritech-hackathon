                                                    <th scope="row"><%= i+1 %></th>
                                                    <td><%= data.land[0].expenditure[i].date %></td>
                                                    <td><%= data.land[0].expenditure[i].name %></td>
                                                    <td><%= data.land[0].expenditure[i].quantity %></td>
                                                    <td><%= data.land[0].expenditure[i].rate %></td>
