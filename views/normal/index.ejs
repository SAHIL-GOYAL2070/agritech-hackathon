                                        <div class="mx-2">
                                            <p class="mb-0 text-light">User1</p>
                                        </div>
                                        <div class="rounded-circle user-image-logo"></div>
                                    <%}else{%>
                                        <div class="xs-btn-wraper">
                                            <a href="/auth/signup/" class="xs-btn">
                                                Sign Up
                                            </a>
                                        </div>
                                    <%}%>
                                </div>
                            </div>
                        </div>
                    </nav>
                </div>
            </div>
        </div>
    </header>

    <section class="xs-banner-sec owl-carousel banner-slider">

        <div class="banner-slider-item banner-item1"
            style="background-image: url(/assets/images/banner-slider/banner_slider_1.jpg);">
            <div class="slider-table">
                <div class="slider-table-cell">
